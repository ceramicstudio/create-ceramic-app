{
  "name": "@ceramicstudio/create-ceramic-app",
  "version": "0.1.0",
  "description": "oclif example Hello World CLI",
  "author": "3Box Labs",
  "bin": {
    "create-ceramic-app": "bin/run.js"
  },
  "homepage": "https://github.com/Sterahi/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sterahi/hello-world.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1",
    "axios": "^0.27.2",
    "chalk": "4.1.2",
    "crypto": "^1.0.1",
    "degit": "^2.8.4",
    "dids": "^3.4.0",
    "inquirer": "^9.1.1",
    "key-did-provider-ed25519": "^2.0.1",
    "key-did-resolver": "^2.1.3",
    "ora": "^6.1.2",
    "uint8arrays": "^3.1.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@swc/core": "^1.3.1",
    "@types/chai": "^4",
    "@types/degit": "^2.8.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "execa": "^6.1.0",
    "globby": "11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "create-ceramic",
    "dirname": "create-ceramic",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build:clean": "shx rm -rf dist",
    "build:js": "swc src -d ./dist --config-file .swcrc",
    "build": "npm run build:clean && npm run build:js",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/ceramicstudio/create-ceramic-app/issues"
  },
  "keywords": [
    "oclif",
    "ceramic",
    "composedb"
  ],
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  }
}
